script MulberryWoods_EventScript_SeviperEncounter {
    playmoncry(SPECIES_SEVIPER, CRY_MODE_NORMAL)
    waitmoncry
    delay(30)
    playse(SE_PIN)
    applymovement(LOCALID_PLAYER, moves(emote_question_mark))
    waitmovement(LOCALID_PLAYER)
    msgbox("...Huh?\n" "That sounded like a Pokémon!", MSGBOX_AUTOCLOSE)
    if(var(VAR_0x8008) == 0) {
        applymovement(LOCALID_PLAYER, moves(walk_right * 2, walk_up * 2))
        waitmovement(LOCALID_PLAYER)
    } elif (var(VAR_0x8008) == 1) {
        applymovement(LOCALID_PLAYER, moves(walk_right * 2, walk_up * 3))
        waitmovement(LOCALID_PLAYER)
    } elif (var(VAR_0x8008) == 2) {
        applymovement(LOCALID_PLAYER, moves(walk_right * 2, walk_up * 4))
        waitmovement(LOCALID_PLAYER)
    } elif (var(VAR_0x8008) == 3) {
        applymovement(LOCALID_PLAYER, moves(walk_right * 2, walk_up * 5))
        waitmovement(LOCALID_PLAYER)
    }
    playse(SE_PIN)
    applymovement(12, moves(face_player, emote_exclamation_mark))
    waitmovement(0)
    delay(72)
    msgbox("(There's that black tag from before.\n" "This time it's wrapped around its tail.)", MSGBOX_AUTOCLOSE)
    applymovement(LOCALID_PLAYER, moves(walk_in_place_up))
    waitmovement(LOCALID_PLAYER)
    delay(72)
    playse(SE_PIN)
    applymovement(12, moves(emote_question_mark))
    waitmovement(0)
    delay(90)
    applymovement(12, moves(walk_in_place_left))
    waitmovement(0)
    delay(60)
    msgbox("... ... ... ... ...{PAUSE 84} ... ... ... ... ... ... ...", MSGBOX_AUTOCLOSE)
    delay(48)
    msgbox("(You removed the tag.)", MSGBOX_AUTOCLOSE) 
    playse(SE_PIN)
    applymovement(12, moves(face_player, emote_exclamation_mark))
    waitmovement(0)
    delay(106)
    playse(SE_PIN)
    applymovement(12, moves(emote_heart))
    waitmovement(0)
    delay(120)
    applymovement(LOCALID_PLAYER, moves(lock_facing_direction, walk_down, unlock_facing_direction))
    waitmovement(LOCALID_PLAYER)
    applymovement(12, moves(walk_down, walk_right * 4, delay_16, face_left))
    waitmovement(0)
    applymovement(LOCALID_PLAYER, moves(walk_in_place_right))
    waitmovement(LOCALID_PLAYER)
    delay(90)
    playmoncry(SPECIES_SEVIPER, CRY_MODE_NORMAL)
    waitmoncry
    delay(60)
    applymovement(12, moves(walk_up * 2, walk_right * 6))
    waitmovement(0)
    removeobject(12)
    setvar(VAR_MET_PLYCHRME_STARTERS, 2)
    setflag(FLAG_HIDE_SEVIPER)
    delay(90)
    releaseall
}

