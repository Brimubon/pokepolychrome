script MulberryWoods_EventScript_GruntEncounter {
    setvar(VAR_MET_PLYCHRME_STARTERS, 3)
    playmoncry(SPECIES_ZANGOOSE, CRY_MODE_NORMAL)
    waitmoncry
    playmoncry(SPECIES_SEVIPER, CRY_MODE_NORMAL)
    waitmoncry
    delay(48)
    playse(SE_PIN)
    applymovement(LOCALID_PLAYER, moves(emote_exclamation_mark))
    waitmovement(LOCALID_PLAYER)
    if(var(VAR_0x8008) == 0) {
        applymovement(LOCALID_PLAYER, moves(walk_up * 4, walk_left * 4))
        waitmovement(LOCALID_PLAYER)
    } elif (var(VAR_0x8008) == 1) {
        applymovement(LOCALID_PLAYER, moves(walk_up * 3, walk_left * 3, walk_up, walk_left * 2))
        waitmovement(LOCALID_PLAYER)
    } elif (var(VAR_0x8008) == 2) {
        applymovement(LOCALID_PLAYER, moves(walk_up * 3, walk_left * 4, walk_up, walk_left * 2))
        waitmovement(LOCALID_PLAYER)
    }
    delay(90)
    applymovement(13, moves(lock_facing_direction, walk_left, unlock_facing_direction))
    waitmovement(0)
    delay(60)
    playse(SE_WALL_HIT)
    applymovement(13, moves(walk_faster_right * 2))
    applymovement(14, moves(delay_16, lock_facing_direction, walk_right, unlock_facing_direction))
    waitmovement(0)
    delay(72)
    playse(SE_WALL_HIT)
    applymovement(14, moves(walk_fast_left))
    applymovement(13, moves(delay_16, lock_facing_direction, walk_left, unlock_facing_direction))
    waitmovement(0)
    delay(60)
    playmoncry(SPECIES_SEVIPER, CRY_MODE_NORMAL)
    waitmoncry
    playmoncry(SPECIES_ZANGOOSE, CRY_MODE_NORMAL)
    waitmoncry
    applymovement(LOCALID_PLAYER, moves(walk_slow_left))
    waitmovement(LOCALID_PLAYER)
    playse(SE_PIN)
    applymovement(13, moves(emote_double_exclamation_mark))
    applymovement(14, moves(face_right, emote_double_exclamation_mark))
    waitmovement(0)
    delay(90)
    playse(SE_PIN)
    applymovement(14, moves(emote_heart))
    applymovement(13, moves(emote_heart, walk_fast_right, walk_fast_down, walk_fast_right * 3, face_up))
    waitmovement(0)
    delay(60)
    applymovement(LOCALID_PLAYER, moves(face_down, delay_16 * 2, face_left))
    waitmovement(LOCALID_PLAYER)
    msgbox("(... ... ...Hm?)\p" "(These Pokémon are carrying\n" "Poké Balls...)", MSGBOX_AUTOCLOSE)
    delay(90)
    addobject(18)
    clearflag(FLAG_HIDE_GRUNT_MULWOODS)
    applymovement(18, moves(walk_up * 5, delay_16 * 2, walk_in_place_down))
    waitmovement(0)
    msgbox(MulberryWoods_EventScript_GruntPreBattle1, MSGBOX_AUTOCLOSE)
    applymovement(18, moves(walk_left * 3, walk_up, walk_left, delay_16 * 2))
    waitmovement(0)    
    playse(SE_PIN)
    applymovement(18, moves(emote_question_mark))
    waitmovement(0)
    msgbox("Hm?\n" "...Wait a minute.", MSGBOX_AUTOCLOSE)
    playse(SE_PIN)
    applymovement(LOCALID_PLAYER, moves(emote_exclamation_mark, walk_in_place_right))
    applymovement(13, moves(emote_exclamation_mark, walk_in_place_right))
    applymovement(14, moves(emote_exclamation_mark))
    waitmovement(0)
    fadeoutbgm(0)
    delay(30)
    playbgm(MUS_HG_TEAM_ROCKET_HQ, FALSE)
    msgbox(MulberryWoods_EventScript_GruntPreBattle2, MSGBOX_AUTOCLOSE)
    delay(48)
    applymovement(LOCALID_PLAYER, moves(lock_facing_direction, walk_slow_left, unlock_facing_direction))
    applymovement(13, moves(delay_8, lock_facing_direction, walk_left, unlock_facing_direction))
    applymovement(14, moves(delay_16, lock_facing_direction, walk_slow_left, unlock_facing_direction))
    waitmovement(0)
    applymovement(18, moves(delay_16 * 2, walk_left))
    waitmovement(0)
    message(MulberryWoods_EventScript_GruntPreBattle3)
    waitmessage
    delay(30)
    dynmultichoice(0, 0, TRUE, 6, 0, DYN_MULTICHOICE_CB_NONE, "Hand over that package!", "(Try to imitate a Pokémon's cry, loudly.)")
    if(var(VAR_RESULT) == 0) {
        msgbox(MulberryWoods_EventScript_GruntHandPackage, MSGBOX_AUTOCLOSE)
    } else {
        closemessage
        delay(60)
        msgbox(MulberryWoods_EventScript_GruntConfused, MSGBOX_AUTOCLOSE)
    }
    delay(48)
    applymovement(18, moves(walk_in_place_right, delay_16 * 2, walk_in_place_left))
    waitmovement(0)
    msgbox(MulberryWoods_EventScript_GruntPreBattle4, MSGBOX_AUTOCLOSE)
    applymovement(13, moves(face_up))
    playmoncry(SPECIES_ZANGOOSE, CRY_MODE_NORMAL)
    waitmoncry
    playmoncry(SPECIES_SEVIPER, CRY_MODE_NORMAL)
    waitmoncry
    playse(SE_PIN)
    applymovement(LOCALID_PLAYER, moves(walk_in_place_down, emote_question_mark, delay_16, walk_in_place_left))
    waitmovement(LOCALID_PLAYER)
    delay(60)
    msgbox("(... ... ... ...Oh!)\p" "(These Pokemon...\n" "They want to join your side!)", MSGBOX_AUTOCLOSE)
    delay(90)
    playse(SE_BALL_OPEN)
    applymovement(14, moves(enter_pokeball))
    applymovement(13, moves(enter_pokeball))
    waitmovement(0)
    removeobject(13)
    removeobject(14)
    delay(90)
    setflag(FLAG_SYS_POKEMON_GET)
    givemon(SPECIES_ZANGOOSE, 5, nature=NATURE_JOLLY, hpIv=31, atkIv=31, defIv=31, speedIv=31, spAtkIv=31, spDefIv=31)
    givemon(SPECIES_SEVIPER, 5, nature=NATURE_IMPISH, hpIv=31, atkIv=31, defIv=31, speedIv=31, spAtkIv=31, spDefIv=31)
    message("Zangoose and Seviper have joined\n" "your party!")
    waitmessage
    playfanfare(MUS_HG_EVOLVED)
    waitfanfare
    closemessage
    fadeoutbgm(0)
    delay(90)
    applymovement(18, moves(walk_in_place_slow_left))
    waitmovement(0)
    msgbox(MulberryWoods_EventScript_GruntPreBattle5, MSGBOX_AUTOCLOSE)
    delay(48)
    playbgm(MUS_HG_ENCOUNTER_ROCKET, FALSE)
    playse(SE_LEDGE)
    applymovement(18, moves(jump_in_place_left, walk_in_place_fast_left * 10))
    applymovement(LOCALID_PLAYER, moves(walk_in_place_right))
    waitmovement(0)
    msgbox(MulberryWoods_EventScript_GruntPreBattle6, MSGBOX_AUTOCLOSE)
    setflag(FLAG_NO_WHITEOUT)
    trainerbattle_no_intro(TRAINER_GRUNT_MULBERRY_WOODS, MulberryWoods_Text_NoWay)
    goto(MulberryWoods_EventScript_GruntPostBattle)
    end
}

script MulberryWoods_EventScript_GruntPostBattle {
    applymovement(18, moves(lock_facing_direction, walk_slow_right, unlock_facing_direction))
    waitmovement(0)
    specialvar(VAR_RESULT, GetBattleOutcome)
    if(var(VAR_RESULT) == B_OUTCOME_LOST) {
        msgbox(MulberryWoods_EventScript_GruntPostBattleLost, MSGBOX_AUTOCLOSE)
    } else {
        msgbox(MulberryWoods_EventScript_GruntPostBattleText1, MSGBOX_AUTOCLOSE)
    }
    clearflag(FLAG_NO_WHITEOUT)
    delay(60)
    setspeaker(SP_NAME_SCARLETT)
    msgbox(MulberryWoods_EventScript_GruntPostBattleScarlett1, MSGBOX_AUTOCLOSE)
    playse(SE_PIN)
    applymovement(18, moves(emote_exclamation_mark, walk_in_place_right))
    waitmovement(0)
    msgbox(MulberryWoods_EventScript_GruntPostBattleText2, MSGBOX_AUTOCLOSE)
    applymovement(18, moves(walk_in_place_fast_left))
    waitmovement(0)
    msgbox(MulberryWoods_EventScript_GruntPostBattleText3, MSGBOX_AUTOCLOSE)
    applymovement(18, moves(walk_left * 2))
    waitmovement(0)
    //give player hawthorne's parcel item here once I make it
    delay(60)
    applymovement(18, moves(lock_facing_direction, walk_right, unlock_facing_direction))
    waitmovement(0)
    msgbox(MulberryWoods_EventScript_GruntPostBattleText4, MSGBOX_AUTOCLOSE)
    applymovement(18, moves(walk_fast_right * 4, walk_fast_down * 7))
    waitmovement(0)
    removeobject(18)
    delay(90)
    setspeaker(SP_NAME_SCARLETT)
    msgbox(MulberryWoods_EventScript_GruntPostBattleScarlett2, MSGBOX_AUTOCLOSE)
    delay(90)
    setobjectxyperm(9, 52, 27)
    clearflag(FLAG_HIDE_SCARLETT_MULWOODS1)
    addobject(9)
    applymovement(9, moves(walk_up * 5, walk_in_place_left, delay_16 * 3, walk_fast_left * 3, walk_fast_up, walk_fast_left * 2))
    waitmovement(0)
    setspeaker(SP_NAME_SCARLETT)
    msgbox(MulberryWoods_EventScript_GruntPostBattleScarlett3, MSGBOX_AUTOCLOSE)
    applymovement(9, moves(walk_left))
    waitmovement(0)
    fadescreenswapbuffers(FADE_TO_BLACK)
    special(HealPlayerParty)
    playfanfare(MUS_HEAL)
    waitfanfare
    delay(48)
    fadescreenswapbuffers(FADE_FROM_BLACK)
    applymovement(9, moves(lock_facing_direction, walk_right, unlock_facing_direction))
    waitmovement(0)
    setspeaker(SP_NAME_SCARLETT)
    msgbox(MulberryWoods_EventScript_GruntPostBattleScarlett4, MSGBOX_AUTOCLOSE)
    applymovement(9, moves(walk_right * 4, walk_down * 7))
    waitmovement(0)
    removeobject(9)
    delay(90)
    //clear flag for no wild battles here
    msgbox(MulberryWoods_EventScript_GruntPostBattleTagBreaks, MSGBOX_AUTOCLOSE)
    setvar(VAR_MULBERRY_TOWN_STATE, 2)
    setvar(VAR_HAWTH_LAB_STATE, 2)
    releaseall
}

