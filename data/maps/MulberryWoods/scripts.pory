script MulberryWoods_EventScript_Scarlett {
    playse(SE_PIN)
    applymovement(9, moves(walk_in_place_down, emote_exclamation_mark))
    waitmovement(0)
    setspeaker(SP_NAME_SCARLETT)
    msgbox(MulberryWoods_EventScript_ScarlettText1, MSGBOX_AUTOCLOSE)
    if(var(VAR_0x8008) == 0) {
        applymovement(LOCALID_PLAYER, moves(walk_up * 3, walk_in_place_left))
        waitmovement(LOCALID_PLAYER)
    } elif (var(VAR_0x8008) == 1) {
        applymovement(LOCALID_PLAYER, moves(walk_up, walk_right, walk_up * 2, walk_in_place_left))
        waitmovement(LOCALID_PLAYER)
    }
    applymovement(9, moves(walk_in_place_right))
    waitmovement(0)
    setspeaker(SP_NAME_SCARLETT)
    msgbox(MulberryWoods_EventScript_ScarlettText2, MSGBOX_AUTOCLOSE)
    applymovement(9, moves(walk_in_place_left))
    waitmovement(0)
    setspeaker(SP_NAME_SCARLETT)
    msgbox(MulberryWoods_EventScript_ScarlettText3, MSGBOX_AUTOCLOSE)
    applymovement(9, moves(walk_in_place_right))
    waitmovement(0)
    setspeaker(SP_NAME_SCARLETT)
    msgbox(MulberryWoods_EventScript_ScarlettText4, MSGBOX_AUTOCLOSE)
    applymovement(9, moves(walk_left * 8))
    waitmovement(0)
    removeobject(9)
    setflag(FLAG_HIDE_SCARLETT_MULWOODS1)
    applymovement(LOCALID_PLAYER, moves(walk_in_place_right))
    waitmovement(0)
    setvar(VAR_MULBERRY_TOWN_GRUNT, 2)
    releaseall
}

